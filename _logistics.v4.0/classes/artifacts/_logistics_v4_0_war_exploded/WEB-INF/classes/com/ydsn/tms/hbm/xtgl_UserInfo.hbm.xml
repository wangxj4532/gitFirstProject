<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.ydsn.tms.po">
	<class name="xtgl_UserInfo" table="userInfo">
		<id name="user_id" column="user_id"></id>
		<property name="user_mch" column="user_mch"></property>
		<property name="user_name" column="user_name"></property>
		<property name="user_pwd" column="user_pwd"></property>
		<property name="stm_id" column="stm_id"></property>
	</class>
</hibernate-mapping>